pipeline {
    agent any
    stages {
        stage('check out the code') {
            steps {
                echo 'check out the code in my git repo.'
                
            }
        }
        stage('Build') {
            steps {
                echo 'Building.'
                
                
            }
        }
        stage('Test') {
            steps {
                echo 'Testing.'
                
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying.'
                
                
            }
        }
    }
}
